//2 Count Common Elements Between Two Arrays
#include <stdio.h>

int main() {
    int nums1[] = {2,3,2};
    int nums2[] = {1,2};
    int n = 3, m = 2;

    int count1 = 0, count2 = 0;

    for(int i=0;i<n;i++)
        for(int j=0;j<m;j++)
            if(nums1[i] == nums2[j]){
                count1++;
                break;
            }

    for(int i=0;i<m;i++)
        for(int j=0;j<n;j++)
            if(nums2[i] == nums1[j]){
                count2++;
                break;
            }

    printf("[%d,%d]", count1, count2);
}
