//1 First Palindromic String in Array
#include <stdio.h>
#include <string.h>

int isPalindrome(char *str) {
    int l = 0, r = strlen(str) - 1;
    while (l < r) {
        if (str[l] != str[r])
            return 0;
        l++;
        r--;
    }
    return 1;
}

int main() {
    char words[][20] = {"abc","car","ada","racecar","cool"};
    int n = 5;

    for(int i = 0; i < n; i++) {
        if(isPalindrome(words[i])) {
            printf("%s\n", words[i]);
            return 0;
        }
    }

    printf("\"\"\n");
    return 0;
}
